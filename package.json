{
  "name": "termin8",
  "version": "1.0.0",
  "description": "Backend for Apps wishing to provide a Calendar App to arrange meetings between individuals",
  "main": "app.js",
  "scripts": {
    "start": "nodemon server.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coderwurst/termin8.git"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      ".git",
      "node_modules/**/node_modules"
    ],
    "delay": "3",
    "verbose": true,
    "autoAttachChildProcesses": true,
    "execMap": {
      "js": "node --harmony"
    },
    "ext": "js,json"
  },
  "keywords": [],
  "author": "coderwurst (andrew mcmechan)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/coderwurst/termin8/issues"
  },
  "homepage": "https://github.com/coderwurst/termin8#readme",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "moment": "^2.24.0",
    "mongoose": "^5.8.11"
  },
  "devDependencies": {
    "nodemon": "^2.0.2",
    "jest": "^25.1.0",
    "supertest": "^4.0.2"
  }
}
